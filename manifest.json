{
  "author": "Dima PM",
  "manifest_version": 2,
  "name": "RARBG Auto Thumbnails",
  "version": "0.2.0",
  "description": "Shows torrent thumbnails automatically.",

  "content_scripts": [
    {
      "matches": [ "https://rarbgmirror.org/torrents.php*",
                   "https://rarbgmirror.org/torrent/*",
                   "https://rarbgmirror.org/torrents?category=*",
                   "https://rarbgmirror.org/s/*/",
                   "https://rarbgmirror.org/top10",
                   "https://rarbgmirror.org/top100*",

                   "https://rarbgproxy.org/torrents.php*",
                   "https://rarbgproxy.org/torrent/*",
                   "https://rarbgproxy.org/torrents?category=*",
                   "https://rarbgproxy.org/s/*/",
                   "https://rarbgproxy.org/top10",
                   "https://rarbgproxy.org/top100*",

                   "https://rarbgprx.org/torrents.php*",
                   "https://rarbgprx.org/torrent/*",
                   "https://rarbgprx.org/torrents?category=*",
                   "https://rarbgprx.org/s/*/",
                   "https://rarbgprx.org/top10",
                   "https://rarbgprx.org/top100*",

                   "https://rarbgunblocked.org/torrents.php*",
                   "https://rarbgunblocked.org/torrent/*",
                   "https://rarbgunblocked.org/torrents?category=*",
                   "https://rarbgunblocked.org/s/*/",
                   "https://rarbgunblocked.org/top10",
                   "https://rarbgunblocked.org/top100*",

                   "https://rarbgaccessed.org/torrents.php*",
                   "https://rarbgaccessed.org/torrent/*",
                   "https://rarbgaccessed.org/torrents?category=*",
                   "https://rarbgaccessed.org/s/*/",
                   "https://rarbgaccessed.org/top10",
                   "https://rarbgaccessed.org/top100*"
      ],
      "js": [ "content.js" ]
    }
  ]
}
